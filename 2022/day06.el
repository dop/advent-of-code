(let ((seq-unique-p
       (lambda (seq)
         (cl-loop for (a b) on (sort seq #'<) by #'cdr
                  never (and a b (= a b))))))
  (cl-loop for i from 0 below (- (length aoc-input) 4)
           when (funcall seq-unique-p (string-to-list (seq-subseq aoc-input i (+ i 4))))
           return (+ i 4))) ;; 1582



